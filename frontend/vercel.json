{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "version": 2,
  "build": {
  },
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
